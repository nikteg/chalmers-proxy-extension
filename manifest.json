{
  "name": "Chalmers Library Proxy Redirector",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Use this extension to proxy a e-resource through the Chalmers Library Proxy.",
  "icons": {
    "192": "images/icon-192x192.png",
    "512": "images/icon-512x512.png"
  },
  "background": {
    "scripts": ["scripts/background.js"]
  },
  "permissions": ["http://*/*", "https://*/*"],
  "browser_action": {
    "default_icon": {
      "192": "images/icon-192x192.png",
      "512": "images/icon-512x512.png"
    }
  }
}
